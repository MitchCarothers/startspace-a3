{
    ":root":  {
        "--searchHeight": "40px",
        "--engineOffset": "2px",
        "--engineIconOffset": "18px",
        "--engineSearchHeight": "calc((var(--searchHeight) - 30px - var(--engineOffset)) - var(--engineIconOffset))"
    },
    ".searchForm > :first-child": {
        "border-style": "solid",
        "border-right-style": "none",
        "border-top-left-radius": "20px",
        "border-bottom-left-radius": "20px",
        "border-color": "#AAAAAA",
        "border-width": "1px"
    },
    ".searchForm > :last-child": {
        "border-style": "solid",
        "border-left-style": "none",
        "border-top-right-radius": "20px",
        "border-bottom-right-radius": "20px",
        "border-color": "#AAAAAA",
        "border-width": "1px"
    },
    ".searchForm > :not(:first-child):not(:last-child)": {
        "border-style": "solid",
        "border-left-style": "none",
        "border-right-style": "none",
        "border-color": "#AAAAAA",
        "border-width": "1px"
    },
    ".searchForm": {
        "display": "flex",
        "width": "400px",
        "height": "var(--searchHeight)"
    },
    ".searchText": {
        "flex": "1",
        "color": "#000000",
        "font-size": "16px",
        "outline": "none",
        "min-width": "10px",
        "padding": "0px 0px",
        "font-family": "",
        "padding-left": "0px"
    },
    "::-webkit-input-placeholder": {
        "color": "#737373"
    },
    ":-moz-placeholder": {
        "color": "#737373"
    },
    ":-ms-input-placeholder": {
        "color": "#737373"
    },
    "::placeholder": {
        "color": "#737373"
    },
    ".searchButton": {
        "background-color": "#61e927",
        "width": "50px",
        "display": "flex",
        "align-items": "center",
        "justify-content": "center",
        "font-family": "arial",
        "font-size": "12px",
        "color": "#000000"
    },
    ".searchButton:hover": {
        "opacity": "80%",
        "background-color": ""
    },
    ".searchIcon": {
    },
    ".searchCancel": {
        "display": "flex",
        "align-items": "center",
        "justify-content": "center",
        "background-color": "rgb(0,0,0,0)",
        "padding-right": "5px"
    },
    ".searchCancelButton": {
        "display": "flex",
        "align-items": "center",
        "justify-content": "center",
        "background-color": "rgb(0,0,0,0)",
        "border": "none",
        "border-radius": "40px",
        "width": "28px",
        "height": "28px"
    },
    ".searchCancelButton:hover": {
        "background-color": "rgb(0,0,0,.1)"
    },
    ".engineSelectContainer": {
        "border": "none"
    },
    ".engineSelectButton": {
        "width": "38px",
        "background-color": "transparent",
        "display": "flex",
        "justify-content": "center",
        "align-items": "start",
        "margin-top": "calc(var(--engineSearchHeight) / 2)",
        "border": "none",
        "padding": "0px",
        "margin": "none"
    },
    ".engineSelectButton > .engineIcon": {
        "margin": "10px 0px"
    },
    ".engineSelect": {
        "width": "28px",
        "display": "flex",
        "align-items": "center",
        "justify-content": "center",
        "flex-direction": "column",
        "gap": "10px",
        "background-color": "rgb(2,0,0,0)",
        "padding": "10px",
        "border-radius": "30px",
        "border": "none"
    },
    ".engineIcon": {
        "opacity": "75%",
        "z-index": "1"
    },
    ".engineIcon:hover": {
        "opacity": "100%"
    },
    "body": {
        "display": "flex",
        "align-items": "center",
        "justify-content": "center",
        "height": "700px"
    }
}
