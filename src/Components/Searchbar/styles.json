{
    ".searchForm > :first-child": {
        "border-style": "solid",
        "border-right-style": "none",
        "border-top-left-radius": "20px",
        "border-bottom-left-radius": "20px",
        "border-color": "#AAAAAA",
        "border-width": "1px"
    },
    ".searchForm > :last-child": {
        "border-style": "solid",
        "border-left-style": "none",
        "border-top-right-radius": "20px",
        "border-bottom-right-radius": "20px",
        "border-color": "#AAAAAA",
        "border-width": "1px"
    },
    ".searchForm > :not(:first-child):not(:last-child)": {
        "border-style": "solid",
        "border-left-style": "none",
        "border-right-style": "none",
        "border-color": "#AAAAAA",
        "border-width": "1px"
    },
    ".searchForm": {
        "display": "flex",
        "width": "300px",
        "height": "35px"
    },
    ".searchText": {
        "flex": "1",
        "padding": "0px 10px",
        "font-size": "16px",
        "outline": "none"
    },
    ".searchButtonRight": {
        "background-color": "#F0F0F0",
        "width": "50px",
        "display": "flex",
        "align-items": "center",
        "justify-content": "center",
        "font-family": "arial",
        "font-size": "12px"
    },
    ".searchButtonRight:hover": {
        "opacity": "80%"
    }
}
